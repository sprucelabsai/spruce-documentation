{
  "name": "spruce-documentation",
  "version": "1.0.0",
  "description": "Documentation for Sprucebot!",
  "main": "index.js",
  "scripts": {
    "dev": "eleventy --serve & npx tailwindcss -i assets/css/index.css -c tailwind.config.js -o _site/index.css --watch",
    "build": "ELEVENTY_PRODUCTION=true eleventy && NODE_ENV=production npx tailwindcss -i assets/css/index.css -c tailwind.config.js -o _site/index.css --minify"
  },
  "author": "Talha Faisal",
  "dependencies": {
    "@11ty/eleventy": "^1.0.2",
    "eleventy-plugin-toc": "^1.1.5",
    "markdown-it": "^13.0.2",
    "markdown-it-anchor": "^8.6.7"
  },
  "devDependencies": {
    "@11ty/eleventy-navigation": "^0.3.5",
    "@tailwindcss/typography": "^0.5.7",
    "tailwindcss": "^3.1.8"
  }
}